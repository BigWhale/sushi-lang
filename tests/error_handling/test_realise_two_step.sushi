
fn get_value(i32 should_error) i32:
    if (should_error == 1):
        return Result.Err()
    return Result.Ok(123)

fn main() i32:
    let i32 val1 = get_value(0).realise(0)
    let i32 val2 = get_value(1).realise(-1)

    println("val1 = {val1}")
    println("val2 = {val2}")

    return Result.Ok(0)
