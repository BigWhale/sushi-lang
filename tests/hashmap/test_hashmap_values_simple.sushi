# TEST_TYPE: runtime
# EXPECT_RUNTIME_EXIT: 0

# Test HashMap.values() iterator with foreach loop
fn main() i32:
    let HashMap<string, i32> ages = HashMap.new()
    ages.insert("Alice", 30)
    ages.insert("Bob", 25)
    ages.insert("Charlie", 35)

    # Iterate over values and sum them
    let i32 sum = 0
    foreach(age in ages.values()):
        print(age)
        sum := sum + age

    # Should sum to 30 + 25 + 35 = 90
    if (sum == 90):
        return Result.Ok(0)

    return Result.Ok(1)
