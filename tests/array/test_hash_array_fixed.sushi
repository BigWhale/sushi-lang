# Test hash method for fixed arrays

fn main() i32:
    # Test basic fixed array hashing
    let i32[3] arr1 = [10, 20, 30]
    let u64 hash1 = arr1.hash()
    println("Fixed array [10, 20, 30].hash() = {hash1}")

    # Test that same values produce same hash
    let i32[3] arr2 = [10, 20, 30]
    let u64 hash2 = arr2.hash()
    if (hash1 == hash2):
        println("✓ Same fixed array values produce same hash")
    else:
        println("✗ Same fixed array values produce different hash")

    # Test that different values produce different hash
    let i32[3] arr3 = [5, 15, 25]
    let u64 hash3 = arr3.hash()
    if (hash1 != hash3):
        println("✓ Different fixed array values produce different hashes")
    else:
        println("✗ Different fixed array values produce same hash")

    # Test that element order matters
    let i32[3] arr4 = [30, 20, 10]
    let u64 hash4 = arr4.hash()
    if (hash1 != hash4):
        println("✓ Element order matters ([10,20,30] != [30,20,10])")
    else:
        println("✗ Element order doesn't matter")

    # Test that array length affects hash
    let i32[2] arr5 = [10, 20]
    let u64 hash5 = arr5.hash()
    if (hash1 != hash5):
        println("✓ Array length affects hash ([10,20,30] != [10,20])")
    else:
        println("✗ Array length doesn't affect hash")

    # Note: String arrays not yet fully implemented
    # TODO: Fix string array hashing

    return Result.Ok(0)
