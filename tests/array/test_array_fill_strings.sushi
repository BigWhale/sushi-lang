fn run_test() i32:
    # Test fill() with strings - Don't Panic!
    let string[] towels = from(["dirty", "wet", "lost"])
    towels.fill("clean")

    # Verify all towels are clean
    if (towels.get(0)?? != "clean"):
        return Result.Err(StdError.Error)
    if (towels.get(1)?? != "clean"):
        return Result.Err(StdError.Error)
    if (towels.get(2)?? != "clean"):
        return Result.Err(StdError.Error)

    return Result.Ok(0)

fn main() i32:
    let i32 result = run_test().realise(99)
    return Result.Ok(result)
