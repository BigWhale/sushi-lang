# TEST_TYPE: runtime
# EXPECT_RUNTIME_EXIT: 99
# Test dynamic array bounds checking (RE2020)
# This test verifies that accessing an out-of-bounds index triggers a runtime error
fn run_test() i32:
  let i32[] numbers = from([10, 20, 30])

  # This should fail at runtime - index 5 is out of bounds for array of size 3
  print(numbers.get(5)??)

  return Result.Ok(0)

fn main() i32:
    let i32 result = run_test().realise(99)
    return Result.Ok(result)
