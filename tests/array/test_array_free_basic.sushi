fn main() i32:
    # Test Array.free() - should clear array but keep it usable
    let i32[] arr = from([1, 2, 3])
    println("Before free: len={arr.len()}")

    arr.free()
    println("After free: len={arr.len()}")

    # Array should still be usable after free
    arr.push(10)
    arr.push(20)
    println("After pushes: len={arr.len()}")

    if (arr.len() == 2):
        println("Array.free() works correctly!")
        return Result.Ok(0)

    return Result.Ok(1)
