# This test should FAIL because we try to use a moved dynamic array
# Error should be CE2405: cannot borrow moved variable

fn main() i32:
    let i32[] arr1 = from([1, 2, 3])
    let i32[] arr2 = new()

    # Move arr1 to arr2
    arr2 := arr1

    # This should error - arr1 has been moved
    println(arr1.len())

    return Result.Ok(0)
