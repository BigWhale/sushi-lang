# ERROR TEST: CE2505
# Test that directly assigning Result<T[]> to T[] without unwrapping is an error

fn create_array() i32[]:
    let i32[] arr = from([1, 2, 3])
    return Result.Ok(arr)

fn run_test() i32:
    # This should fail: trying to assign Result<i32[]> to i32[] without ??
    let i32[] numbers = create_array()

    return Result.Ok(0)

fn main() i32:
    let i32 result = run_test().realise(99)
    return Result.Ok(result)
