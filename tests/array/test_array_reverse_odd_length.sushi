fn run_test() i32:
    # Test reverse() with odd length - Arthur Dent's tea problem
    let i32[] tea_attempts = from([1, 2, 3, 4, 5, 6, 7])
    tea_attempts.reverse()

    # Verify order is reversed
    if (tea_attempts.get(0)?? != 7):
        return Result.Err()
    if (tea_attempts.get(1)?? != 6):
        return Result.Err()
    if (tea_attempts.get(2)?? != 5):
        return Result.Err()
    if (tea_attempts.get(3)?? != 4):
        return Result.Err()
    if (tea_attempts.get(4)?? != 3):
        return Result.Err()
    if (tea_attempts.get(5)?? != 2):
        return Result.Err()
    if (tea_attempts.get(6)?? != 1):
        return Result.Err()

    return Result.Ok(0)

fn main() i32:
    let i32 result = run_test().realise(99)
    return Result.Ok(result)
