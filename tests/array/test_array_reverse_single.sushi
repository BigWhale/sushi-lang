fn run_test() i32:
    # Test reverse() on single element - Deep Thought's answer
    let i32[] answer = from([42])
    answer.reverse()

    # Verify element is unchanged
    if (answer.get(0)?? != 42):
        return Result.Err(StdError.Error)
    if (answer.len() != 1):
        return Result.Err(StdError.Error)

    return Result.Ok(0)

fn main() i32:
    let i32 result = run_test().realise(99)
    return Result.Ok(result)
