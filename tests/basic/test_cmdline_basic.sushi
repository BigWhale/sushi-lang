# TEST_TYPE: runtime
# EXPECT_RUNTIME_EXIT: 0
# EXPECT_STDOUT_CONTAINS: test_cmdline_basic

fn run_test(string[] args) i32:
    let i32 i = 0
    while (i < args.len()):
        println(args.get(i)??)
        i := i + 1
    return Result.Ok(0)

fn main(string[] args) i32:
    let i32 result = run_test(args).realise(99)
    return Result.Ok(result)
