# TEST_TYPE: runtime
# EXPECT_RUNTIME_EXIT: 0
# Test modulus operator (%)

fn main() i32:
    # Basic modulus tests
    let i32 mod1 = 10 % 3   # Should be 1
    let i32 mod2 = 15 % 4   # Should be 3
    let i32 mod3 = 20 % 5   # Should be 0
    let i32 mod4 = 7 % 2    # Should be 1

    # Verify results
    if (mod1 != 1):
        return Result.Ok(1)
    if (mod2 != 3):
        return Result.Ok(2)
    if (mod3 != 0):
        return Result.Ok(3)
    if (mod4 != 1):
        return Result.Ok(4)
    # Test with negative numbers
    let i32 neg_mod = 10 % 3  # Should be 1 (same as positive)
    if (neg_mod != 1):
        return Result.Ok(5)
    # Test in expressions
    let i32 complex_mod = (20 + 5) % (3 + 1)  # 25 % 4 = 1
    if (complex_mod != 1):
        return Result.Ok(6)
    return Result.Ok(0)
