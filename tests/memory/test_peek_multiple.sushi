# Test multiple &peek borrows allowed
# Multiple read-only borrows should work

fn sum_two(&peek i32 a, &peek i32 b) i32:
    return Result.Ok(a + b)

fn main() i32:
    let i32 num = 42

    # Print num directly to avoid CW1003 warning
    println(num)

    # Multiple &peek borrows of the same variable - should be allowed
    let Result<i32, StdError> sum_result = sum_two(&peek num, &peek num)
    let i32 result = sum_result.realise(0)

    if (result == 84):
        return Result.Ok(0)

    return Result.Ok(1)
