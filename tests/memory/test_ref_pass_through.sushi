# Test: Passing a reference parameter as a reference to another function
# Simplified test to debug the issue

fn inner(&i32 x) ~:
    x := x + 1
    return Result.Ok(~)

fn outer(&i32 y) ~:
    y := y + 0
    inner(&y)
    return Result.Ok(~)

fn main() i32:
    let i32 n = 10
    outer(&n)
    println(n)
    return Result.Ok(0)
