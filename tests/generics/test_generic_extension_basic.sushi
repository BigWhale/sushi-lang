# TEST_TYPE: runtime
# EXPECT_RUNTIME_EXIT: 0
# EXPECT_STDOUT_CONTAINS: 42

# Test basic generic extension methods
# This test verifies that:
# 1. Generic extension methods can be defined (extend Box<T> unwrap() T)
# 2. They are correctly monomorphized for concrete types (Box<i32>)
# 3. Method calls work correctly with UFCS

struct Box<T>:
    T value

extend Box<T> unwrap() T:
    return self.value

fn main() i32:
    let Box<i32> b = Box(42)
    let i32 result = b.unwrap()
    println(result)
    return Result.Ok(0)

