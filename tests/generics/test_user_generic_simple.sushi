# TEST_TYPE: runtime
# EXPECTED_OUTPUT: Both: 42 and Thanks for all the fish.

# Simplest possible test of user-defined generic enum

enum Pair<T, U>:
    Both(T, U)
    Left(T)
    Right(U)
    Neither()

fn main() i32:
    # Create a Pair<i32, string>
    let Pair<i32, string> p = Pair.Both(42, "Thanks for all the fish.")

    # Pattern match to extract values
    match p:
        Pair.Both(num, text) ->
            println("Both: {num} and {text}")
        Pair.Left(num) ->
            println("Left: {num}")
        Pair.Right(text) ->
            println("Right: {text}")
        Pair.Neither() ->
            println("Neither")

    return Result.Ok(0)
