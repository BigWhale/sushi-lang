# Test: Wrong number of arguments should fail

fn identity<T>(T x) T:
    return Result.Ok(x)

fn run_test() i32:
    # Error: too many arguments - identity expects 1, got 2
    let i32 a = identity(1, 2)??
    return Result.Ok(0)

fn main() i32:
    let i32 result = run_test().realise(99)
    return Result.Ok(result)
