# TEST_TYPE: runtime
# EXPECTED_OUTPUT: Double wrapped

# Test same generic type nested within itself
# "The Answer to the Ultimate Question of Life, The Universe, and Everything is... 42"

fn main() i32:
    # Maybe nested within Maybe
    let Maybe<Maybe<i32>> double_maybe = Maybe.Some(Maybe.Some(42))

    # Result nested within Result
    let Result<Result<string, StdError>, StdError> double_result = Result.Ok(Result.Ok("Mostly Harmless"))

    # Verify both work
    if (double_maybe.is_some()):
        if (double_result):
            println("Double wrapped")
            return Result.Ok(0)

    return Result.Ok(1)
