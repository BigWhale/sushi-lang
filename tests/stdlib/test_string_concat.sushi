# Test string.concat() method with runtime validation
use <io/stdio>
use <collections/strings>

fn main() i32:
    # Test 1: ASCII + ASCII
    let string s1 = "Hello"
    let string s2 = " World"
    let string result1 = s1.concat(s2)
    println(result1)  # Expected: Hello World

    # Test 2: UTF-8 + UTF-8
    let string s3 = "café"
    let string s4 = "世界"
    let string result2 = s3.concat(s4)
    println(result2)  # Expected: café世界

    # Test 3: Empty string + non-empty
    let string empty = ""
    let string s5 = "test"
    let string result3 = empty.concat(s5)
    println(result3)  # Expected: test

    # Test 4: Non-empty + empty string
    let string result4 = s5.concat(empty)
    println(result4)  # Expected: test

    # Test 5: Empty + empty
    let string result5 = empty.concat(empty)
    let bool is_empty = result5.is_empty()
    if (is_empty):
        println("empty")  # Expected: empty

    return Result.Ok(0)
