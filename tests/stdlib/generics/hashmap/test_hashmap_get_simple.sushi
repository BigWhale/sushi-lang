use <collections/hashmap>
fn main() i32:
    let HashMap<i32, i32> map = HashMap.new()

    # Insert a value
    map.insert(42, 100)

    # Get the value back
    let Maybe<i32> result = map.get(42)
    match result:
        Maybe.Some(val) ->
            println("Found: {val}")
        Maybe.None() ->
            println("Not found")

    # Try to get a non-existent key
    let Maybe<i32> result2 = map.get(99)
    match result2:
        Maybe.Some(val) ->
            println("Found unexpected: {val}")
        Maybe.None() ->
            println("Key 99 not found (expected)")

    return Result.Ok(0)
