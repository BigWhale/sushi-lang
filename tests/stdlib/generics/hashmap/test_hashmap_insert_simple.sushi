# Test simple HashMap insert with i32 keys

use <collections/hashmap>
fn main() i32:
    # Create empty HashMap
    let HashMap<i32, string> map = HashMap.new()

    # Try to insert one entry
    map.insert(42, "answer")

    # Check size
    let i32 size = map.len()
    println("Size after insert: {size}")

    if (size != 1):
        println("ERROR: Expected size 1, got {size}")
        return Result.Ok(1)

    println("Insert test passed!")
    return Result.Ok(0)
