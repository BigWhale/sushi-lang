use <collections/hashmap>
fn main() i32:
    let HashMap<string, i32> map = HashMap.new()

    # Insert some values
    map.insert("alice", 30)
    map.insert("bob", 25)

    # Test contains_key
    if (map.contains_key("alice")):
        println("alice found (expected)")

    if (map.contains_key("bob")):
        println("bob found (expected)")

    if (map.contains_key("charlie")):
        println("charlie found (unexpected!)")
    else:
        println("charlie not found (expected)")

    return Result.Ok(0)
