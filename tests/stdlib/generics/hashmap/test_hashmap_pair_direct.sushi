# Test HashMap with Pair<T, U> keys - direct construction

use <collections/hashmap>
struct Pair<T, U>:
    T first
    U second

fn main() i32:
    # Test: Pair<i32, string> as key
    let HashMap<Pair<i32, string>, bool> map = HashMap.new()

    # Construct Pair explicitly with type
    let Pair<i32, string> p1 = Pair(1, "one")
    map.insert(p1, true)

    # Use explicit variable for get() argument
    let Pair<i32, string> p2 = Pair(1, "one")
    match map.get(p2):
        Maybe.Some(found) -> println("Pair key works: {found}")
        Maybe.None() -> println("ERROR: Should have found pair")

    println("Test passed")
    return Result.Ok(0)
