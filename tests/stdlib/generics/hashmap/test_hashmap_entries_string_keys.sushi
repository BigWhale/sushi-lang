# TEST_TYPE: runtime
# EXPECT_RUNTIME_EXIT: 0

# Test HashMap.entries() with string keys and i32 values

use <collections/hashmap>
fn main() i32:
    let HashMap<string, i32> ages = HashMap.new()
    ages.insert("Alice", 30)
    ages.insert("Bob", 25)

    let i32 total = 0
    foreach(entry in ages.entries()):
        total := total + entry.value

    # 30 + 25 = 55
    if (total == 55):
        return Result.Ok(0)

    return Result.Ok(1)
