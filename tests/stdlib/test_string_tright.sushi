# TEST_TYPE: runtime
# EXPECT_RUNTIME_EXIT: 0
# EXPECT_STDOUT: hello
# EXPECT_STDOUT:   world
# EXPECT_STDOUT:   test
# EXPECT_STDOUT:
# EXPECT_STDOUT:
# EXPECT_STDOUT: hello
# EXPECT_STDOUT: ðŸ‘‹

use <collections/strings>

fn main() i32:
    # Test 1: Trim trailing whitespace
    let string s1 = "hello  "
    let string r1 = s1.tright()
    println(r1)  # Expected: hello

    # Test 2: No trailing whitespace
    let string s2 = "  world"
    let string r2 = s2.tright()
    println(r2)  # Expected:   world

    # Test 3: Both leading and trailing whitespace
    let string s3 = "  test  "
    let string r3 = s3.tright()
    println(r3)  # Expected:   test

    # Test 4: Empty string
    let string s4 = ""
    let string r4 = s4.tright()
    println(r4)  # Expected: (empty line)

    # Test 5: Only whitespace
    let string s5 = "   "
    let string r5 = s5.tright()
    println(r5)  # Expected: (empty line)

    # Test 6: Multiple whitespace types (space, tab, carriage return)
    let string s6 = "hello \t\r"
    let string r6 = s6.tright()
    println(r6)  # Expected: hello

    # Test 7: UTF-8 with trailing whitespace
    let string s7 = "ðŸ‘‹  "
    let string r7 = s7.tright()
    println(r7)  # Expected: ðŸ‘‹

    return Result.Ok(0)
