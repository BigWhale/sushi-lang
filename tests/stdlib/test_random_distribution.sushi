use <random>

fn main() i32:
    println("=== Random Distribution Test ===")
    println("")
    println("Generating 100 random values in range [1, 10)...")

    let i32 i = 0
    while (i < 100):
        rand_range(1, 10)
        i := i + 1

    let u64 r1 = rand()
    let u64 r2 = rand()

    println("")
    println("Sample u64 values:")
    println("  {r1}")
    println("  {r2}")

    let f64 f1 = rand_f64()
    let f64 f2 = rand_f64()
    let f64 f3 = rand_f64()

    println("")
    println("Sample f64 values in [0.0, 1.0):")
    println("  {f1}")
    println("  {f2}")
    println("  {f3}")

    if (f1 >= 0.0 and f1 < 1.0 and f2 >= 0.0 and f2 < 1.0 and f3 >= 0.0 and f3 < 1.0):
        println("")
        println("Pass: All f64 values in valid range")

    println("")
    println("=== Distribution Test Complete ===")
    return Result.Ok(0)
