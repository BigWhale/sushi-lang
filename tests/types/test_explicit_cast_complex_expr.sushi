# TEST_TYPE: runtime
# EXPECT_RUNTIME_EXIT: 0
# Complex expressions with explicit casts should pass
fn main() i32:
  let i32 a = 10
  let i16 b = 20 as i16
  let f64 c = 5.5

  # Multiple casts in one expression
  let f64 result = ((a + (b as i32)) as f64) * c

  # Result should be (10 + 20) * 5.5 = 165.0
  if (result > 164.0 and result < 166.0):
    return Result.Ok(0)

  return Result.Ok(1)
