# Test wildcard pattern in match


enum Color:
    Red
    Green
    Blue
    Yellow
    Orange

fn test_color(Color c) i32:
    match c:
        Color.Red ->
            return Result.Ok(1)
        Color.Green ->
            return Result.Ok(2)
        _ ->
            return Result.Ok(0)

fn main() i32:
    let Color r = Color.Red()
    let i32 v1 = test_color(r).realise(0)
    # v1 = 1

    let Color b = Color.Blue()
    let i32 v2 = test_color(b).realise(0)
    # v2 = 0 (caught by wildcard)

    let Color y = Color.Yellow()
    let i32 v3 = test_color(y).realise(0)
    # v3 = 0 (caught by wildcard)

    # Return v1 + v2 + v3 = 1 + 0 + 0 = 1
    let i32 result = v1 + v2 + v3
    let i32 expected = 1
    return Result.Ok(result - expected)
