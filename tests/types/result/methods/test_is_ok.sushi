# EXPECT_STDOUT_CONTAINS: PASS: is_ok returns true for Ok
# EXPECT_STDOUT_CONTAINS: PASS: is_ok returns false for Err
# EXPECT_STDERR_EMPTY: true

# Tests is_ok method

enum MyError:
    Error

fn get_ok() i32 | MyError:
    return Result.Ok(42)

fn get_err() i32 | MyError:
    return Result.Err(MyError.Error)

fn main() i32:
    let Result<i32, MyError> ok_result = get_ok()
    if (ok_result.is_ok()):
        println("PASS: is_ok returns true for Ok")
    else:
        println("FAIL: is_ok returned false for Ok")
        return Result.Ok(1)

    let Result<i32, MyError> err_result = get_err()
    if (not err_result.is_ok()):
        println("PASS: is_ok returns false for Err")
        return Result.Ok(0)
    else:
        println("FAIL: is_ok returned true for Err")
        return Result.Ok(1)
