# TEST_TYPE: runtime
# EXPECT_RUNTIME_EXIT: 0
# All arithmetic operators with explicit casts should pass
fn main() i32:
  let i32 a = 20
  let i16 b = 5 as i16

  let i32 sum = a + (b as i32)
  let i32 diff = a - (b as i32)
  let i32 prod = a * (b as i32)
  let i32 quot = a / (b as i32)
  let i32 rem = a % (b as i32)

  # a=20, b=5: sum=25, diff=15, prod=100, quot=4, rem=0
  if (sum == 25 and diff == 15 and prod == 100 and quot == 4 and rem == 0):
    return Result.Ok(0)

  return Result.Ok(1)
