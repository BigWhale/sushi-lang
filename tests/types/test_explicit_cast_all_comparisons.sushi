# TEST_TYPE: runtime
# EXPECT_RUNTIME_EXIT: 0
# All comparison operators with explicit casts should pass
fn main() i32:
  let i32 x = 10
  let i16 y = 5 as i16

  let bool eq = x == (y as i32)
  let bool neq = x != (y as i32)
  let bool gt = x > (y as i32)
  let bool gte = x >= (y as i32)
  let bool lt = x < (y as i32)
  let bool lte = x <= (y as i32)

  # x=10, y=5: eq=false, neq=true, gt=true, gte=true, lt=false, lte=false
  if (not eq and neq and gt and gte and not lt and not lte):
    return Result.Ok(0)

  return Result.Ok(1)
