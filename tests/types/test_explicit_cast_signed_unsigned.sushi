# TEST_TYPE: runtime
# EXPECT_RUNTIME_EXIT: 0
# Explicit casts between signed/unsigned should pass
fn main() i32:
  let i32 signed = 42
  let u16 unsigned = 100 as u16

  # Cast unsigned to signed for comparison
  if (signed < (unsigned as i32)):
    # Cast unsigned to signed for arithmetic
    let i32 result = signed + (unsigned as i32)
    if (result == 142):
      return Result.Ok(0)

  return Result.Ok(1)
