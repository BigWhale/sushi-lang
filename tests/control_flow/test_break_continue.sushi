# EXPECT_RUNTIME_EXIT: 0
# TEST_TYPE: runtime
fn main() i32:
  let i32 i = 0
  let i32 sum = 0
  while (i < 10):
    i := i + 1
    if (i == 5):
      continue
    if (i == 8):
      break
    sum := sum + i
  # sum should be 1+2+3+4+6+7 = 23
  if (sum == 23):
    return Result.Ok(0)
  else:
    return Result.Ok(1)
