# Test CE4006: Missing one of multiple constraints

perk Hashable:
    fn hash() u64

perk Displayable:
    fn display() string

struct Point:
    i32 x
    i32 y

# Only implements Hashable, not Displayable
extend Point with Hashable:
    fn hash() u64:
        return 42 as u64

fn debug_value<T: Hashable + Displayable>(T value) ~:
    let u64 h = value.hash()
    let string s = value.display()
    println(s)
    println(h)
    return Result.Ok(~)

fn run_test() i32:
    let Point p = Point(10, 20)
    # Should fail: Point doesn't implement Displayable
    debug_value(p)??
    return Result.Ok(0)

fn main() i32:
    let i32 result = run_test().realise(99)
    return Result.Ok(result)
