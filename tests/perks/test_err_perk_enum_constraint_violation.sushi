# Test CE4006: Generic enum constraint violation

perk Hashable:
    fn hash() u64

struct Point:
    i32 x
    i32 y

# Point does NOT implement Hashable

enum Option<T: Hashable>:
    Some(T)
    None()

fn main() i32:
    let Point p = Point(10, 20)
    # Should fail: Point doesn't satisfy Hashable constraint
    let Option<Point> opt = Option.Some(p)
    return Result.Ok(0)
