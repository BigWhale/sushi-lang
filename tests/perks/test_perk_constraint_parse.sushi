# Test parsing of perk constraints in function signatures

perk Hashable:
    fn hash() u64

struct Point:
    i32 x
    i32 y

extend Point with Hashable:
    fn hash() u64:
        return 42 as u64

# Function with single constraint
fn compute_hash<T: Hashable>(T value) u64:
    return value.hash()

fn main() i32:
    return Result.Ok(0)
